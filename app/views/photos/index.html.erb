<div class="jumbotron vertical-center">
      <div class="container conrainer-bg">
      <p id="notice"><%= notice %></p>

      <h1>Photos</h1>

      <table>
        <thead>
          <tr>
            <th>Title</th>
            <th>Description</th>
            <th>Image file name</th>
            <th>Image content type</th>
            <th>Image file size</th>
            <th>Author</th>
            <th colspan="3"></th>
          </tr>
        </thead>

        <tbody>
          <% @photos.each do |photo| %>
            <tr>
              <td><%= photo.title %></td>
              <td><%= photo.description %></td>
              <td><%= photo.image_file_name %></td>
              <td><%= photo.image_content_type %></td>
              <td><%= photo.image_file_size %></td>
              <td><%= photo.author %></td>
              <td><%= link_to 'Show', photo %></td>
              <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
              <td><%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
          <% end %>
        </tbody>
      </table>

      <br>

      <%= link_to 'New Photo', new_photo_path %>
  </div>
</div>